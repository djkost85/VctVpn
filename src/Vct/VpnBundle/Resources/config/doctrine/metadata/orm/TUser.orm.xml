<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TUser" table="t_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="vpnServerId" type="string" column="vpn_server_id" length="32" nullable="true"/>
    <field name="email" type="string" column="email" length="32" nullable="true"/>
    <field name="password" type="string" column="password" length="32" nullable="true"/>
    <field name="registerDate" type="date" column="register_date" nullable="true"/>
    <field name="paidDate" type="date" column="paid_date" nullable="true"/>
    <field name="paidFee" type="integer" column="paid_fee" nullable="true"/>
    <field name="expireDate" type="date" column="expire_date" nullable="true"/>
    <field name="expireFlag" type="boolean" column="expire_flag" nullable="true"/>
    <field name="createTime" type="date" column="create_time" nullable="true"/>
    <field name="updateTime" type="date" column="update_time" nullable="true"/>
  </entity>
</doctrine-mapping>
